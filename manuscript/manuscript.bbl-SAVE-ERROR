      \strng{namehash}{3954df14df58650990e3de8c3e578191}
      \strng{fullhash}{e366e12321713aa34c639c1b237d6589}
      \strng{bibnamehash}{e366e12321713aa34c639c1b237d6589}
      \strng{authorbibnamehash}{e366e12321713aa34c639c1b237d6589}
      \strng{authornamehash}{3954df14df58650990e3de8c3e578191}
